      <div class="col-12 mt-2 ">
        <div class="card">
            <div class="card-header">
                <h6><button class="btn bg-gradient-secondary mt-3  waves-effect waves-light" onclick="toggleDiv()"><i class="fa fa-search"></i>Filter Result</button></h6>
            </div>
            <div class="card-body mb-4" style="display: none;" id="myDiv">
                <div class="col-md-8 offset-md-2">
                  <div class="form-group">
                      <select  class="form-control" id="planSelect" onchange="updateGoals()">
                          <option value="0" >please select plan</option>
                       <% if(allplans.length >0 ){%>
                          <% allplans.forEach(function(row){ %>
                          
                           <option value="<%= row.planid %>"><%= row.plantitle %></option>
                          <% }) %>
                       <%} %>
                        </select>
                  </div>
                  <div class="form-group">
                      <select id="goalSelect" onchange="updateDirection()"class="form-control">
                          <option value="0" >please select strategic goal</option>
                          <!-- Options will be dynamically populated here -->
                        </select>
                  </div>
                  <div class="form-group">
        
                      <select id="directionSelect" class="form-control" onchange="updateMajor()">
                          <option value="0" >please select strategic direction</option>
                          <!-- Options will be dynamically populated here -->
                        </select>
                  </div>
                  <div class="form-group">
        
                      <select id="majorActivitySelect" class="form-control" onchange="updateDetail()">
                          <option value="0" >please select major activity</option>
                          <!-- Options will be dynamically populated here -->
                        </select>
                  </div>
                  <div class="form-group">
        
                      <select id="detailActivitySelect" class="form-control">
                          <option value="0" >please select detail activity</option>
                          <!-- Options will be dynamically populated here -->
                        </select>
                        
                  </div>
                 
                  <script>
                      function updateGoals() {
                        var planSelect = document.getElementById("planSelect");
                        var goalSelect = document.getElementById("goalSelect");
                        var selectedPlanId = planSelect.value;
                    
                        // Clear previous options
                        removeAllOptions(goalSelect);
                        // Fetch goals based on the selected plan ID
                        fetch('/stvcpms/goals/' + selectedPlanId)
                          .then(response => response.json())
                          .then(goals => {
                            // Generate new options based on the fetched goals
                            goals.forEach((goal) => {
                              var option = document.createElement("option");
                              option.value = goal.sgoalid;
                              option.textContent = goal.sgoaltitle;
                              goalSelect.appendChild(option);
                            });
                          })
                          .catch(error => {
                            console.log(error);
                          });
                      }
                    
                      function updateDirection() {
                        var goalSelect = document.getElementById("goalSelect");
                        var directionSelect = document.getElementById("directionSelect");
                        var goalSelect = goalSelect.value;
                        removeAllOptions(directionSelect);
                        // Clear previous options     // Fetch major activities based on the selected goal ID
                        fetch('/stvcpms/direction/' + goalSelect)
                          .then(response => response.json())
                          .then(majorActivities => {
                            // Generate new options based on the fetched major activities
                            majorActivities.forEach((majorActivity) => {
                              var option = document.createElement("option");
                              option.value = majorActivity.sdirid;
                              option.textContent = majorActivity.sdirtitle;
                              directionSelect.appendChild(option);
                            });
                          })
                          .catch(error => {
                              console.log(error);
                          });
                      }
                      
                      function updateMajor() {
                        var directionSelect = document.getElementById("directionSelect");
                        var majorActivitySelect = document.getElementById("majorActivitySelect");
                        var goalSelect = directionSelect.value;
                        removeAllOptions(majorActivitySelect);
                        // Clear previous options
                     
                        // Fetch major activities based on the selected goal ID
                        fetch('/stvcpms/major-activities/' + goalSelect)
                          .then(response => response.json())
                          .then(majorActivities => {
                            // Generate new options based on the fetched major activities
                            majorActivities.forEach((majorActivity) => {
                              var option = document.createElement("option");
                              option.value = majorActivity.mactivityid;
                              option.textContent = majorActivity.mactivitytitle;
                              majorActivitySelect.appendChild(option);
                            });
                          })
                          .catch(error => {
                              console.log(error);
                          });
                      }
                      
                      function updateDetail() {
                        var majorSelect = document.getElementById("majorActivitySelect");
                        var detailActivitySelect = document.getElementById("detailActivitySelect");
                        var majorSelectval = majorSelect.value;
                    
                        // Clear previous options
                     
                        removeAllOptions(detailActivitySelect);
                        // Fetch major activities based on the selected goal ID
                        fetch('/stvcpms/detail-activities/' + majorSelectval)
                          .then(response => response.json())
                          .then(majorActivities => {
                            // Generate new options based on the fetched major activities
                            majorActivities.forEach((majorActivity) => {
                              var option = document.createElement("option");
                              option.value = majorActivity.dactivityid;
                              option.textContent = majorActivity.dactivitytitle;
                              detailActivitySelect.appendChild(option);
                            });
                          })
                          .catch(error => {
                              console.log(error);
                          });
                      }
                      function removeAllOptions(selectElement) {
          var i;
          for (i = selectElement.options.length - 1; i >= 1; i--) {
            selectElement.remove(i);
          }
        }
      
                    </script>
                    
                    
