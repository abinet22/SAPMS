                  <div class="col-lg-12">
                    <% if(dact){ %>
                      <div class="d-flex flex-column h-100">
                        <p class="mb-1 pt-2 text-bold" style="color: #007bff; font-size: 18px; font-weight: bold;">
                          <i class="fas fa-info-circle"></i> Report Infos
                        </p>
                        <h5 class="font-weight-bolder" style="color: #333; font-size: 24px;"><i class="fas fa-bookmark"></i> Activity Title: <%= dact.dactivitytitle %></h5>
                        <h5 class="font-weight-bolder" style="color: #333; font-size: 20px;"><i class="fas fa-tag"></i> Activity Code: <%= dact.dactivitycode %></h5>
                        <p class="mb-2" style="color: #555; font-size: 16px;"><i class="fas fa-file-alt"></i> <strong>Discription:</strong> <%= dact.dactivitydescription %></p>
                        <p class="mb-2"><strong>Registered Risk</strong> &nbsp;<%= dact.dactivityrisks %> </p>
                        <p class="mb-2" style="color: #555; font-size: 16px;"><i class="fas fa-calendar-day"></i> Duration: <%= dact.dactivityduration %> Days</p>
                        <p class="text-body text-sm font-weight-bold mb-0" style="color: #555; font-size: 14px;">
                          <i class="fas fa-calendar"></i> From:
                          <span class="ml-1">
                            <%= new Date(plan.startdate).toLocaleDateString() %> 
                            <i class="fas fa-long-arrow-alt-right mx-1"></i> 
                            <%= new Date(plan.enddate).toLocaleDateString() %>
                          </span>
                        </p>
                      </div>
                    <% } %>
                  </div>
                </div>
              </div>
            <div class=" p-5">
              <div class="timeline timeline-one-side" style="width: 100%;">
                <% if(finalrpt.length >0){ %>
                    <% var j = 0 ;%>
                 <% finalrpt.forEach(function (row){%>
                      <% j++ %>
                      <div class="">
                       
                    
                        <div class="card-body pt-4 p-3">
                         
                          <ul class="list-group">
                            <li class="list-group-item border-0 d-flex p-4 mb-2 bg-gray-100 border-radius-lg">
                              <div class="d-flex flex-column">
                                <h6 class="mb-3 text-sm">Report By: &nbsp; <%= row.targetgroupname %>/<%= row.grouptype %>/<%= row.reporttype %></h6>
                          
                                <span class="mb-2 text-xs">Indicator: <span class="text-dark font-weight-bold ms-sm-2"><%= row.dacttindicator %></span></span>
                                <span class="mb-2 text-xs">Input: <span class="text-dark ms-sm-2 font-weight-bold"><%= row.dacttinputtype %></span></span>
                                <span class="text-xs">KPI: <span class="text-dark ms-sm-2 font-weight-bold"><%= row.dactkpi %></span></span>
                               

                                Score Value:<p><%= row.number %></p>
                                <p><%= row.percent %></p>
                                <p><%= row.ratio %></p>
                                <div class="col-md-12" style="width: 100%;">
                                    <label class="btn-outline-info"> Report:- </label> <p><%= row.progressreport %>
                                   
                                     
                                     </p>
                                    <label class="btn-outline-info"> Remark:-   </label> <p><%= row.remark %></p>
                                      
                                    <hr class="horizontal dark mt-0">
                                     </div>
                                    
                                     <div class="card-footer float-lg-end">
                     
                                      Report Date:- <p class="mt-2"><%= new Date(row.reportdate).toLocaleDateString() %></p>
                                    
                                     </div>
                              </div>
                              <!-- <div class="ms-auto text-end">
                                <a class="btn btn-link text-danger text-gradient px-3 mb-0" href="javascript:;"><i class="far fa-trash-alt me-2" aria-hidden="true"></i>Delete</a>
                                <a class="btn btn-link text-dark px-3 mb-0" href="javascript:;"><i class="fas fa-pencil-alt text-dark me-2" aria-hidden="true"></i>Edit</a>
                              </div> -->
                            </li>
                          
                          </ul>
                        </div>
                      </div>
                 
                 
                    <%})}else{}%>
                
