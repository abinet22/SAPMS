          <option value="0">Please Select Output Here</option>
          <% if(mact.length>0){ %>
           <% mact.forEach(function(row){ %>
              <option value="<%= row.mactivityid %>"><%= row.mactivitytitle %></option>
           <% })%>
         <% }else {%>
          <label>No Output Where Found</label>
          <%} %>
         </select>
        </div>
        <div class="modal-footer">
          <input type="hidden" value="<%= sgoalid %>" name="sgoalid">
