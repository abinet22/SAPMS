    <div class="container-fluid">
    
      <div class="col-12 ">
        <div class="card mb-4">
          <div class="card-header pb-0 bg-secondary">
            <h6 class="mb-1">Output</h6>
            <p class="text-sm">Existing Outputs </p>
          </div>
          <div class="card-body p-3">
            <div class="row">
             <% if(mact.length >0){%>
             <% mact.forEach(function (row){ %>
              <div class="col-xl-4 col-md-6 mb-xl-0 mb-5 py-5">
                <div class="card card-blog card-plain">
                  <div class="position-relative">
                    <a class="d-block shadow-xl border-radius-xl">
                      <i class="fas fa-cogs fa-3x"></i>     <p class="text-gradient text-dark mb-2 text-sm" style="text-transform: uppercase;"><%= row.mactivitytitle %></p>
                  
                    </a>
                  </div>
                  <div class="card-body px-1 pb-0">
                    <a href="javascript:;" class="d-block text-decoration-none">
                      <h5 class="font-weight-bold">
