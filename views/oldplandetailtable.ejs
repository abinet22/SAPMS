  if (item.children) {
      if (item.type === 'activity2') {
        // Create a table for the activity
        html += createActivityTable(numbering);
        // Render detail KPIs as rows within the table
        html += renderKPIRows(item.children, numbering);
        html += '</tbody>';
        html += '</table>';
        html += '</div>';
